/**
 * List data source for {{name}}.
 * @requires angular
 * @requires apeman
 */
(function (ng, ap) {
    "use strict";

    ng
        .module('ok.datasources')
        .factory('{{Name}}ListingDatasource', function (ListingDatasource, {{Name}}Entity, {{name}}ApiService) {
            return ListingDatasource.define(
                /** @lends {{Name}}ListingDatasource.prototype */
                {
                    _listRequest: function (query, callback) {
                        {{name}}ApiService.list(query, callback);
                    },
                    _parseData: function (data) {
                        return data.map({{Name}}Entity.new);
                    }
                }
            );
        });

})(angular, apeman);