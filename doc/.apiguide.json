{
    "tags": {
        "allowUnknownTags": true
    },
    "plugins": ["plugins/markdown"],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true
        },
        "systemName": "okunishitaka.com",
        "footer": "<script>var h=document.head;h.innerHTML=h.innerHTML+'<link rel=\"shortcut icon\" href=\"../images/favicon.png\"/><style>.navbar .dropdown-menu{max-height:500px;width:360px;}</style>';</script>",
        "copyright": "okunishitaka Copyright Â© 2014",
        "navType": "vertical",
        "theme": "united",
        "linenums": true,
        "collapseSymbols": false,
        "outputSourceFiles":true,
        "highlightTutorialCode":true,
        "dateFormat": "YYYY-MM-DD",
        "inverseNav": false
    },
    "markdown": {
        "parser": "gfm",
        "hardwrap": true
    }
}