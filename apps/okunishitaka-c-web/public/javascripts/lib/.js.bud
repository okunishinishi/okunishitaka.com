"use strict";

var u = require('apeman-util'),
    format = u.core.util.format,
    path = u.core.path,
    fs = u.core.fs,
    data = require('./.js.data.js');

module.exports = data.map(function (src) {
    return {
        mode: '444',
        force: true,
        data: {},
        path: format('%s/%s', __dirname, path.basename(src)),
        tmpl: function () {
            return fs.readFileSync(src);
        }
    }
});



